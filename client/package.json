{
  "name": "react-spree-boilerplate-client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev-server": "NODE_ENV=development webpack-dev-server --config webpack/devServer.babel.js --hot --inline",
    "prebuild": "NODE_ENV=production yarn run build:vendor && yarn run translate",
    "build": "webpack --config webpack/config.babel.js --colors --progress",
    "build:dev": "NODE_ENV=development yarn run build --",
    "build:test": "NODE_ENV=test yarn run build --",
    "build:prod": "NODE_ENV=production yarn run build -- -p",
    "build:vendor": "webpack --config webpack/bundles/vendor.babel.js",
    "build:rails": "webpack --config webpack/bundles/rails.babel.js",
    "build:app": "webpack --config webpack/bundles/app.babel.js",
    "translate": "bundle exec rake react_on_rails:locale",
    "test": "mocha \"./{test,app}/**/*.spec.{js,jsx}\"",
    "lint": "eslint ./app --ext .js,.jsx"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "class-autobind": "^0.1.4",
    "classnames": "^2.2.5",
    "es5-shim": "^4.5.9",
    "es6-error": "^4.0.2",
    "is-iterable": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-form-data": "^1.0.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "querystring": "^0.2.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-intl": "^2.3.0",
    "react-on-rails": "^8.0.3",
    "react-redux": "^5.0.5",
    "react-ultimate-pagination": "^1.0.2",
    "redux": "^3.7.0",
    "redux-saga": "^0.15.3",
    "reselect": "^3.0.1",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.70.0",
    "universal-router": "^3.2.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.3.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-things": "^0.2.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.10.3",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "ignore-styles": "^5.0.1",
    "imports-loader": "^0.7.1",
    "js-yaml": "^3.8.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "mocha": "^3.5.0",
    "nock": "^9.0.14",
    "postcss": "^6.0.2",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^2.0.6",
    "postcss-url": "^6.3.1",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-merge": "^4.1.0"
  },
  "optionalDependencies": {
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.3"
  }
}
